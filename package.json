{
  "name": "n8n-nodes-userpilot",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for UserPilot with 12 resources: Users, Companies, Events, Flows, NPS Surveys, Checklists, Resource Center, Segments, Data Export, Jobs, Spotlights, and Banners. Includes webhook trigger support.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-userpilot.git"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "userpilot",
    "user-onboarding",
    "product-growth",
    "user-tracking",
    "nps",
    "analytics",
    "automation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/UserPilotApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/UserPilot/UserPilot.node.js",
      "dist/nodes/UserPilot/UserPilotTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md",
    "README.md"
  ]
}
